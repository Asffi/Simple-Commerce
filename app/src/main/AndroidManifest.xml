<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.visualcommerce">

    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="face" />
        <activity android:name=".trialroom.FaceFilterActivity"></activity>
        <activity android:name="com.example.visualcommerce.login.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddProductsByAdmin"
            android:exported="true"/>
        <activity android:name=".CartActivity"
            android:exported="true"/>
        <activity android:name=".ConfirmFinalOrderAcivity"
            android:exported="true"/>
        <activity android:name=".SearchActivity2"
            android:exported="true"/>
        <activity android:name="com.example.visualcommerce.Productdetails" />
        <activity android:name="com.example.visualcommerce.MainActivity">

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>